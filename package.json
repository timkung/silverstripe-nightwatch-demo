  "engines": {
    "node": ">=8.11.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "once": "node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
